# Write Python 3 code in this online editor and run it.
class Shape:
    def __init__(self, length):
        self.length=length

    def area (self):
        return 0
        
class Square (Shape):
        
    def __init__(self,length):
        super().__init__(length)
        
    def area (self):
        return self.length * self.length

x = int(input())

sq = Square(x)

print(sq.area())    
    
        
                
